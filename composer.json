{
    "name": "alexmorbo/homeassistant",
    "type": "library",
    "license": "MIT",
    "description": "Library for connect to Home Assistant from symfony dockerized application",
    "minimum-stability": "dev",
    "keywords": ["homeassistant"],
    "authors": [
        {
            "name": "Alex Morbo",
            "email": "alex@morbo.ru"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/http-client": "6.2.*",
        "symfony/serializer": "6.2.*"
    },
    "autoload": {
        "psr-4": {
            "AlexMorbo\\HomeAssistant\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
